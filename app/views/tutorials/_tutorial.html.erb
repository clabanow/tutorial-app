<div class='tutorial'>
  <div>
    <%= link_to tutorial.title, tutorial.url %> |
    Link: <%= tutorial.url %> |
    <% if tutorial.author %>
      Author: <%= tutorial.author %>
    <% end %>
    Author: <%= tutorial.author %> |
    Created: <%= tutorial.date_created.strftime("%B %Y") %>
  </div>
  <div>
    <% if tutorial.is_paid %>
      Paid
    <% else %>
      Free
    <% end %>
  </div>
</div>
<br>